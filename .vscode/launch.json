{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Vss2Git",
      "type": "clr",
      "request": "launch",
      "program": "${workspaceFolder}/Vss2Git/bin/${input:configuration}/net8.0-windows/Vss2Git.exe",
      "preLaunchTask": "build",
      "console": "internalConsole"
    },
    {
      "name": "VssDump",
      "type": "clr",
      "request": "launch",
      "program": "${workspaceFolder}/VssDump/bin/${input:configuration}/net8.0/VssDump.exe",
      "preLaunchTask": "build",
      "console": "internalConsole"
    },
    {
      "name": "HashTest",
      "type": "clr",
      "request": "launch",
      "program": "${workspaceFolder}/HashTest/bin/${input:configuration}/net8.0/HashTest.exe",
      "preLaunchTask": "build",
      "console": "internalConsole"
    }
  ],
  "inputs": [
    {
      "id": "configuration",
      "type": "pickString",
      "description": "Configuration",
      "options": ["Debug", "Release"],
      "default": "Debug"
    }
  ]
}
