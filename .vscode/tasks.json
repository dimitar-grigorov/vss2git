{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-solution",
      "type": "shell",
      "command": "${config:msbuildPath}",
      "args": [
        "Vss2Git.sln",
        "/p:Configuration=Debug"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "silent",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build-vss2git",
      "type": "shell",
      "command": "${config:msbuildPath}",
      "args": [
        "Vss2Git/Vss2Git.csproj",
        "/p:Configuration=Debug"
      ],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "silent",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build-vssdump",
      "type": "shell",
      "command": "${config:msbuildPath}",
      "args": [
        "VssDump/VssDump.csproj",
        "/p:Configuration=Debug"
      ],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "silent",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build-hashtest",
      "type": "shell",
      "command": "${config:msbuildPath}",
      "args": [
        "HashTest/HashTest.csproj",
        "/p:Configuration=Debug"
      ],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "silent",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": "$msCompile"
    },
    {
      "label": "clean",
      "type": "shell",
      "command": "${config:msbuildPath}",
      "args": [
        "Vss2Git.sln",
        "/t:Clean",
        "/p:Configuration=Debug"
      ],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "problemMatcher": "$msCompile"
    }
  ]
}